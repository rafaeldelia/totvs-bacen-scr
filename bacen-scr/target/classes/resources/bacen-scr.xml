<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Registros de entrada/saida para consulta bacenscr -->
<!-- @Author Ricart Monachesi -->
<root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="acesso.xsd">
	<!-- metodo de acesso para ser carregado no acesso -->
	<code name="br.com.totvs.acessos.BacenScr" />

	<!-- esse deverah ser sufixo para ser acrescentado ao nome -->
	<!-- vindo da politica, entao irei buscar na hash da politica -->
	<!-- hash.getKey(cpfcnpj); um de/para para os elementos -->
	<nome_acesso>bacen-scr</nome_acesso>

	<!-- contem os dados necessarios para entrada no SisbacenScr -->
	<entrada>
		<!-- posso testar pelo valor obrigatorio dentro de cada acesso -->
		<field description="CPF ou CNPJ a ser consultado" type="String" format="">CPFCNPJ</field>
		<field description="Tipo do cliente 1-PF 2-PJ" type="String" format="">TIPOCLIENTE</field>
		<field description="Data Base da Consulta YYYY-MM" type="String" format="">DATABASE</field>
		<field description="Usuário SISBACEN" type="String" format="">USUARIOSISBACEN</field>
		<field description="Senha SISBACEN" type="String" format="">SENHASISBACEN</field>
		<field description="Autorização" type="String" format="">AUTORIZACAO</field>
		<field description="Cnpj Instituição Financeira" type="String" format="">CNPJIF</field>
	</entrada>

	<!-- contem todas as saidas disponiveis pelo sisbacenscr -->
	<!-- Obs.: quando counter=alguma_coisa, entao todo o bloco -->
	<!-- abaixo sofrerah um looping baseado nesse counter -->
	<saida id="resumoDoCliente" counter="">
		<!-- esses dois atributos nao entrarao no looping -->
		<register description="Data Base Consultada" type="String" format="">DATABASECONSULTADA</register>                                          
	</saida>
	
</root>